{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "password": {
            "type": "string",
            "minLength": 5,
            "maxLength": 30
        },
        "first-name": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "last-name": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "email": {
            "type": "string",
            "minLength": 5,
            "maxLength": 60,
            "format": "email"
        }
    },
    "additionalProperties": true,
    "required": [
        "email",
        "password",
        "first-name",
        "last-name"
    ]
}